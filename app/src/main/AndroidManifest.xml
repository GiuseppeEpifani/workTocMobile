        <activity android:name=".ui.Cliente.EditarSolicitud.EditarSolicitudTrabajo" />
        <activity android:name=".ui.Cliente.PublicarSolicitud.SplashProcesarSolicitudTrabajo" />
        <activity android:name=".ui.Cliente.PublicarSolicitud.InicioCliente" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="key" />
    </application>

</manifest>